@media (max-width: 767px) {
  .navbar,
  #global-engage-header,
  #global-engage-footer{
    display: none;
  }
}

.audio {

  audio {
    width: 400px;
    background:#333;
    border: 3px #333 solid;
    height: 35px;
    border-radius: 10px;
  }

  #playlist {
    width: 400px;
    border: 3px #333 solid;
    border-radius: 10px;
    background: #333;

    li {
      padding-bottom: 5px;
    }

    a {
      width:374px;
    }
  }

}

